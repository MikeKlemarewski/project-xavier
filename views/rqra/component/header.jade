div#header
	img#logo(src="../images/img.png", alt="logo")
	div#askQuestion Ask a Question
		input#askQuestionInput(type="text")
	div#profile
		div#profiletext
			div#profileGreeting Hey there,
			div#profileName #{user.firstName} #{user.lastName}
		div#profilePicture
			img(src="../images/img.png", alt="profile", onclick="showNotificationMenu()")
			div#newNotificationCount 1
			nav#notificationList
				include ./notificationList
			
script(type="text/javascript", src="../javascripts/rqra/notificationMenu.js")